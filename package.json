{
  "name": "react-tiny-mask",
  "version": "0.0.2",
  "description": "React input mask",
  "keywords": [
    "mask",
    "input",
    "input-mask",
    "react"
  ],
  "author": "Alexander Yanykin",
  "homepage": "https://github.com/rhjje/react-tiny-mask",
  "repository": {
    "url": "https://github.com/rhjje/react-tiny-mask",
    "type": "git"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/react-tiny-mask.umd.js",
  "module": "./dist/react-tiny-mask.es.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/react-tiny-mask.es.js",
      "require": "./dist/react-tiny-mask.umd.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "npm run build",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint src --max-warnings=0"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.29.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.3",
    "prettier": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^3.5.2",
    "vitest": "^0.34.3"
  }
}
